<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_data_name"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昵称"
            android:layout_marginLeft="12dp"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="小太阳"
            android:layout_marginLeft="70dp"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_sex"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="性别"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="11dp"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:text="女"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_birthday"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="生日"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="11dp"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_width="106dp"
            android:layout_height="wrap_content"
            android:text="2000年01月01日"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_position"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginLeft="11dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="位置"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.00km 刚刚"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_liveid"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginLeft="11dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="直播间号"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="123456"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_yixianid"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginLeft="11dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="艺线ID号"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="567894"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_yixian_grade"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginLeft="11dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="艺线等级"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6级"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_integral"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginLeft="11dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="积分"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <TextView
            android:layout_marginLeft="70dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="24（升级还需5小时）"
            android:textColor="#ff000000"
            android:textSize="14dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_data_Privilege"
        android:background="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <TextView
            android:layout_marginLeft="11dp"
            android:layout_marginTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="特权"
            android:textColor="#ff000000"
            android:textSize="12dp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:background="@color/view" />
    </RelativeLayout>


</LinearLayout>
