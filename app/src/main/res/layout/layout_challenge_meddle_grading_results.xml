<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_marginTop="10dp"
    android:layout_height="wrap_content">

    <!--评分结果-->
    <TextView
        android:id="@+id/tv_layout_challenge_meddle_grading_result_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:text="@string/challenge_meddle_grading_results"/>


    <TextView
        android:layout_below="@+id/tv_layout_challenge_meddle_grading_result_title"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/px_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F"
        android:gravity="center"
        android:textSize="50sp"
        android:textColor="#5DD677"
        android:background="@drawable/yjy_weitongguo"
        android:id="@+id/textView5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="@dimen/px_67"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:background="@drawable/icon_challenge_meddle_success_btn"
        android:text="@string/challenge_meddle_grade_success_start" />

    <RelativeLayout
        android:layout_below="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="@dimen/px_36"
        android:layout_height="20dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:text="@string/challenge_meddle_grade_average"/>
            <TextView
                android:layout_marginLeft="3dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="12232"
                android:textColor="#2BC254"
                android:textSize="12sp"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:text="@string/challenge_meddle_grade_likes_number"/>
            <TextView
                android:layout_marginLeft="3dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="12232"
                android:textColor="#2BC254"
                android:textSize="12sp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:text="@string/challenge_meddle_grade_gift_number"/>
            <TextView
                android:layout_marginLeft="3dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="12232"
                android:textColor="#2BC254"
                android:textSize="12sp"
                />

        </LinearLayout>





    </RelativeLayout>










</RelativeLayout>